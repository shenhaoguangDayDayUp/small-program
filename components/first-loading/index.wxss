@import '../../templates/third/weui.wxss';
.sd__first__loading {
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #666;
}
.sd__first__loading-nodata {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.sd__first__loading-nodata image {
  margin-top: 80rpx;
}
.sd__first__loading-nodata--tips {
  font-size: 28rpx;
  color: #666;
  margin-top: 20rpx;
}
.sd__first__loading-nodata-btn {
  font-size: 32rpx;
  margin-top: 48rpx;
  padding: 20rpx 60rpx;
  color: #fff;
  background-color: #934900;
  border-radius: 6rpx;
}
.sd__first__loading-nodata-btn--highlighted {
  color: e7e7e7;
  background-color: #7d4915;
}
.sd__first__loading-nodata--tips {
  position: relative;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.sd__first__loading-nodata--tips .tips {
  position: relative;
  z-index: 2;
  padding: 20rpx 40rpx;
  background-color: #f7f7f7;
}
.sd__first__loading-nodata--tips .line {
  position: absolute;
  right: 60rpx;
  left: 60rpx;
  top: 50%;
  border-top: 1px solid #e7e7e7;
  z-index: 1;
}