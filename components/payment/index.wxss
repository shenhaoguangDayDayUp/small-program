}

.pa {
  position: absolute;
  right: 15rpx;
  top: 50%;
  transform: translateY(-50%);
  display: flex;
  align-content: center;
  justify-content: center;
}

.pay-box {
  z-index: 111;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  overflow-y: hidden;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 999;
}

.pay-box .box-container {
  position: absolute;
  width: 100%;
  height: 400rpx;
  left: 0;
  bottom: 0;
  color: #363636;
  background-color: #fff;
}

.v-1px-t, .v-1px-l, .v-1px-b, .v-1px {
  position: relative;
}

.v-1px-b:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1rpx;
  border-bottom: 1rpx solid #c7c7c7;
  color: #c7c7c7;
  transform-origin: 0 100%;
  transform: scaleY(0.5);
}
.v-1px-t:before {
  z-index: 112;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1rpx;
  content: "";
  border-top: 1rpx solid #c7c7c7;
  transform: scaleY(0.5);
  color: #c7c7c7;
  transform-origin: 0 0;
}

.v-1px-l:before {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  width: 1rpx;
  content: "";
  border-left: 2rpx solid #c7c7c7;
  transform: scaleX(0.5);
  color: #c7c7c7;
  transform-origin: 0 0;
}

.v-1px:before {
  position: absolute;
  left: 0;
  top: 0;
  width: 200%;
  height: 200%;
  content: "";
  border: 1rpx solid #c7c7c7;
  border-left: 2rpx solid #c7c7c7;
  border-right: 2rpx solid #c7c7c7;
  border-bottom: 2rpx solid #c7c7c7;
  transform: scale(0.5);
  color: #c7c7c7;
  transform-origin: left top;
  z-index: 1000;
}


