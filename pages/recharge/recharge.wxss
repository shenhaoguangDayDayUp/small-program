/* pages/recharge/recharge.wxss */
  .contents {
    box-sizing: border-box;
    padding-left: 20rpx;
    padding-right: 20rpx;
    padding-top: 36rpx;
    margin-top:94rpx; 
    color: white;
  }
    .contents .price {
       display: flex;
      flex-direction: row;
      align-items: center;
      height: 30rpx;
      line-height: 30rpx;
      justify-content: flex-start;
      width: 100%;
    }
    .contents .price .price-bar{
        width: 5rpx;
        height: 100%;
        background: #ffcb16;
    }
     .contents .price .title{
        margin-left: 22rpx;
        flex: 1;
     }

    .contents  .price-list {
      padding-bottom: 52rpx;
      padding-top: 17rpx;
      padding-left: 22rpx;
      padding-right: 22rpx;
      display: flex;
      flex-direction: row;
      position: relative;
      flex-wrap: wrap;
      justify-content: space-between;
    }
   .contents  .price-list  .item-active {
        background-image: url("data:image/png;base64,");
        background-repeat: no-repeat;
        background-size: 200rpx 80rpx;
        background-position: 0 0;
        border: 0 !important;
      }
   .contents  .price-list   .item {
        position: relative;
        width: 200rpx;
        height: 80rpx;
        background-color: #23262b;
        color: #e9e9e9;
        font-size: 28rpx;
        margin-top: 28rpx;
        display: flex;
        align-items: center;
        justify-content: center;
        border: 1rpx solid #999999;
        border-radius: 5rpx;
      }
     .contents  .price-list  .item-pay {
        position: relative;
        width: 320rpx;
        height: 80rpx;
        background-color: #23262b;
        color: #e9e9e9;
        font-size: 28rpx;
        margin-top: 28rpx;
        display: flex;
        align-items: center;
        justify-content: center;
        border: 1rpx solid #999999;
        border-radius: 5rpx;
      }
      .contents  .price-list .item-pay-active {
       background-image: url("data:image/png;base64,");
        background-repeat: no-repeat;
        background-size: 320rpx 80rpx;
        background-position: 0 0;
        border: 0 !important;
      }
   .contents   .next {
      margin-top: 50rpx;
      display: flex;
      flex-direction: row;
      height: 80rpx;
      text-align: center;
   }
     .contents  .next  .btn {
        height: 100%;
        line-height: 80rpx;
        flex: 1;
        background: #ffcb16;
        border-radius: 10rpx;
        font-size: 28rpx;
        color: #000000;
      }
    